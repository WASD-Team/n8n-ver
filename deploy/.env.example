# PostgreSQL настройки
POSTGRES_USER=app_user
POSTGRES_PASSWORD=change_me_secure_password
POSTGRES_PORT=5432

# Ключ шифрования (минимум 32 символа, сгенерируй: openssl rand -hex 32)
ENCRYPTION_KEY=change_me_to_random_32_chars_minimum_for_encryption_security

# n8n интеграция
N8N_BASE_URL=https://your-n8n.example.com
N8N_WEBHOOK_URL=https://your-n8n.example.com/webhook/версионирование
N8N_API_KEY=

# Порт веб-приложения (по умолчанию 3000)
WEB_PORT=3000
